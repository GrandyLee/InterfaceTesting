            <stringProp name="script">import static net.javacrumbs.jsonunit.JsonAssert.assertJsonEquals;
import static net.javacrumbs.jsonunit.JsonAssert.when;
import static net.javacrumbs.jsonunit.core.Option.IGNORING_VALUES;
import org.apache.jmeter.services.FileServer;
import java.nio.file.Path;
import java.nio.file.Paths;

/* 
 * 从http请求的注释中提取预期json文件名 
 */
def matcher = (sampler.getComment() =~ /expectjson=\{(.*)\}/);
if (!matcher.find()) {
	return;
}
jsonfilename = matcher[0][1].trim();
//log.info(matcher.group());
//log.info(matcher[0]);


/* 
 * 读取预期json 
 */
String resultJson = prev.getResponseDataAsString();
String expectJson;
try {
	Path jsonpath = Paths.get(FileServer.getFileServer().getBaseDir(), &quot;expectjson&quot;, jsonfilename); 
	expectJson = new File(jsonpath.toString()).getText();
} catch (Exception e) {
	log.error(&quot;fail to open file:&quot; + jsonpath);
	return;
}

//log.info(&quot;quanql-resultJson-&gt;&quot; + resultJson);
//log.info(&quot;quanql-expectJson-&gt;&quot; + expectJson);

/* 
 * 只比较json结构 
 */
try {
	assertJsonEquals(expectJson, resultJson, when(IGNORING_VALUES));
} catch (AssertionError e) {
	AssertionResult.setFailure(true);
	AssertionResult.setFailureMessage(e.getMessage());
}

/* 
 * 比较json值（忽略一些不稳定字段） 
 */
 
</stringProp>
